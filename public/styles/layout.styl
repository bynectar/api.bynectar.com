/* BONES */
.section
  display block
  clear both

.grid
  display block

  &:after
    content " "
    display block
    clear both

// Images
.flexible-image
  width 100%
  height auto

.fadeInOnScroll
  opacity 0
  filter blur(25px)
  transition all ease-out .3s
  &.visible
    opacity 1
    filter blur(0px)